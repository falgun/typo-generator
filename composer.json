{
    "name": "falgunphp/typo-generator",
    "type": "library",
    "homepage": "https://github.com/falgun/typo-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "S.M. Ataur Rahman",
            "email": "dev.ataur.rahman@gmail.com",
            "homepage": "https://ataur.net/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Falgun\\Typo\\Generator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Falgun\\Typo\\Generator\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "falgunphp/kuery": "dev-master"
    },
    "require-dev": {
        "phpro/grumphp": "^1.3",
        "phpunit/phpunit": "^9.5",
        "sempro/phpunit-pretty-print": "^1.4",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.18"
    },
    "bin": ["/bin/generate-typo-meta"],
    "scripts": {
        "phpunit": "phpunit --verbose --colors=always"
    },
    "config": {
        "sort-packages": true
    }
}
